  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Http",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/cmd/server/main.go",
      "env": {
          "POSTGRES_DEFAULT_ID": "postgres_default",
          "POSTGRES_DEFAULT_URL": "postgres://postgres:toor@localhost:5432/todo_service?sslmode=disable",
          "POSTGRES_DEFAULT_TEST_URL": "postgres://postgres:toor@localhost:5432/todo_service_test?sslmode=disable"
      },
      "args": []
    }
  ]
}